<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="productivity.clippy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/paperclip_1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/paperclip_2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".LoggedInActivity"></activity>
        <activity android:name=".LoggedInActivity_debug"></activity>
        <activity android:name=".TNCActivity"></activity>
    </application>

</manifest>